{
  "manifest_version": 3,
  "name": "Tab Manager",
  "version": "1.0",
  "description": "Açık sekmeleri yönetin ve temizleyin.",
  "permissions": [
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Sekme Yöneticisi",
    "default_icon": "icon.svg"
  },
  "icons": {
    "16": "icon.svg",
    "48": "icon.svg",
    "128": "icon.svg"
  }
}
